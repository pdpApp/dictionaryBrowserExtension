{
   "manifest_version": 3,
   "name": "Dictionary",
   "description": "Just double click on the work and get the meaning of it!",
   "version": "1.0",
   "content_scripts": [
     {
       "js": ["scripts/content.js"],
       "matches": ["https://*/*"]
     }
   ],
   "permissions": ["storage"],
   "action": {
     "default_popup": "popup.html",
     "default_icon": "dictionary_icon.png"
   }
 }